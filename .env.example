# Maestro CLI configuration
MAESTRO_BIN=maestro
MAESTRO_DEVICE= # e.g. emulator-5554, leave empty to use default target
MAESTRO_APP_ID=default
MAESTRO_APP_INSTALL_TOOL=xcrun
IOS_SIMULATOR_TARGET=booted
MAESTRO_INSTALL_APP_BEFORE_TEST=true
MAESTRO_INSTALL_APP_ONCE=true
MAESTRO_REINSTALL_APP_PER_SCENARIO=true
APP_SKIP_ONBOARDING_DEEPLINK=

# Optional tuning
MAESTRO_SCREENSHOT_MAX_SIDE_PX=1440
